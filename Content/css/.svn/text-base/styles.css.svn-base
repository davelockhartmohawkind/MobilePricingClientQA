.sort, .sortof { border-right: 1px #bfbbbb solid; cursor:pointer !important; }
.sort.ender, .line.ender, .sortof.ender { border-right: 0; }
.sort.starter, .line.starter, .sortof.starter { padding-left:22px; }

.line { display:block; float:left; }
.line { border-right: 1px rgba(255,255,255,0) solid; }

.sort, .sortof { display:block; float:left; }
.sort.active { background:#444444; }

.sort h5, .sortof h5 { color:#d2d2d3; font-family: 'proxima_nova', 'Helvetica Neue', Helvetica, Arial, sans-serif; font-size:14px; font-weight:400; line-height:16px; padding-top:10px; }


